{
  "name": "@moxb/semui",
  "version": "0.2.0-beta.1",
  "description": "moxb semantic-ui bindings",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "main": "build",
  "typings": "build/",
  "files": [
    "build/",
    "src/**/*.less"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moxb/moxb.git"
  },
  "author": "moxb (moxb.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moxb/moxb/issues"
  },
  "homepage": "https://github.com/moxb/moxb#readme",
  "keywords": [
    "mobx",
    "semantic-ui-react",
    "react",
    "UI"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupTestFrameworkScriptFile": "./jest/jestAdapter.js",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(css|less)$": "./jest/jestMockStyle.js",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "modulePaths": [
      "<rootDir>/node_modules"
    ],
    "testMatch": [
      "**/src/**/__tests__/*.(ts|tsx)"
    ]
  },
  "peerDependencies": {
    "@moxb/moxb": "^0.1.4-beta.5",
    "mobx": "^4.3.0 || ^5.0.0",
    "mobx-react": "^5.2.3",
    "semantic-ui-react": "^0.82.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.13",
    "@types/jest": "^23.3.1",
    "@types/marked": "^0.4.0",
    "@types/meteor": "^1.4.16",
    "@types/react": "^16.4.8",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "jsdom": "^12.0.0",
    "jsdom-global": "^3.0.2",
    "mobx": "^4.3.0 || ^5.0.0",
    "mobx-react": "^5.2.3",
    "pretty": "^2.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "semantic-ui-react": "^0.82.2"
  },
  "dependencies": {
    "@moxb/moxb": "^0.2.0-beta.1",
    "marked": "^0.5.0"
  }
}
