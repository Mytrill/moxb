{
  "name": "@moxb/meteor",
  "version": "0.2.0-beta.7",
  "description": "MobX meteor binding",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "main": "build",
  "typings": "build/",
  "files": [
    "build/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moxb/moxb.git"
  },
  "author": "moxb (moxb.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moxb/moxb/issues"
  },
  "homepage": "https://github.com/moxb/moxb#readme",
  "keywords": [
    "mobx",
    "meteor"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "node",
    "testMatch": [
      "**/src/**/__tests__/*.(ts|tsx)"
    ]
  },
  "peerDependencies": {
    "@moxb/moxb": ">0.2.0-beta.7",
    "mobx": "^4.3.0 || ^5.0.0"
  },
  "devDependencies": {
    "@moxb/moxb": "^0.2.0-beta.7",
    "@types/jest": "^23.3.1",
    "@types/meteor": "^1.4.16",
    "mobx": "^4.3.0 || ^5.0.0"
  }
}
